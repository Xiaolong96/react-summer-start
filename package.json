{
  "name": "react-summer-start",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/pro-layout": "^6.4.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.5.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "analyzer": "ANALYZER=1 craco build",
    "test": "craco test",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint:fix": "npm run lint:js-fix && npm run lint:style-fix && npm run lint:prettier-fix",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:js-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:style-fix": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:prettier-fix": "prettier -c --write \"**/*\"",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "./src/**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{js,jsx,tsx,ts,less,md,json,html}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "craco-less": "^1.17.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "sideEffects": [
    "**/*.less"
  ]
}
